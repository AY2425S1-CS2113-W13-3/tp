@startuml
!include style.puml
hide footbox

participant ":RemoveCommand" as command COMMAND_COLOUR
participant ":EventList" as eventList EVENT_COLOUR

group sd Remove participant
    activate command COMMAND_COLOUR
    command -> eventList : removeParticipantFromEvent(participantName : String, eventName : String)
    activate eventList EVENT_COLOUR

    eventList -> eventList : getEventByName()
    activate eventList EVENT_COLOUR

    return event:Event

    alt event not found
        eventList --> command : :boolean
    else else
        ref over eventList : Remove participant from event
        eventList --> command : :boolean
    end
end


@enduml