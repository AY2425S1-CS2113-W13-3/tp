@startuml
!include style.puml
hide footbox

participant ":Main" as main
participant ":Parser"  as parser  
participant ":XYZCommand" as xyzCommand  

create parser
main -> parser : Parser()
activate parser  
return

main -> parser : parseCommand(command)
activate parser  

parser -> parser : parseXYZCommand(input, commandParts)
activate parser  

create xyzCommand
parser -> xyzCommand : XYZCommand(...)
activate xyzCommand  
return :XYZCommand
return

parser --> main : command:Command
deactivate parser

@enduml